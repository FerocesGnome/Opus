#include <iostream>
#include <math.h>
using namespace std;
bool is_palindrom(int N) {
   int a, b = 0;
   for (a = N; a > 0; a /= 10)
      b = b * 10 + a % 10;
   return b == N;
}
void print_palindrom(int n){
   if (n == 1){
      cout << 9;
      return;
   }
   if (n == 2){
      cout << 99;
      return;
   }
int A = 0;
   if (n % 2 == 0)
      A = pow(10, n - 1) + 1 + 88 * (pow(10, n / 2 - 1));
   else
      A = pow(10, n - 1) + 1 + 7 * (pow(10, n / 2));
int M = pow(10, n);
   for (A; A < M; A += 9) {
      if (is_palindrom(A) == true) {
         cout << A << endl;
      }
   }
}
int main(){
int n;
cout << "Enter the length of the palindromes :";
cin >> n;
print_palindrom(n);
return 0;
}
